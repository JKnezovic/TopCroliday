{
  "expo": {
    "name": "TopCroliday",
    "slug": "TopCroliday",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "primaryColor": "#092240",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#092240"
      },
      "permissions":[],
      "versionCode":1,
      "package":"com.eondevelopers.topcroliday"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
